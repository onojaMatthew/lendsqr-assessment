{
  "name": "lendsqr-assessment",
  "version": "1.0.0",
  "description": "An assessment test",
  "main": "server.js",
  "author": "Onoja Igoche Matthew",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --exec babel-node src/server",
    "seed": "babel-node src/seed.js",
    "clean": "rimraf 'build'",
    "start": "cd build && node server.js",
    "create-migration": "cd build && knex migrate:make create_users_and_transactions_tables",
    "create-seed": "cd build && knex seed:make 01_users && knex seed:make 02_transactions",
    "rollback": "cd build && knex migrate:rollback",
    "migrate-latest": "cd build && knex migrate:latest",
    "run-seed": "cd build && knex seed:run",
    "build": "yarn clean && babel src --out-dir build",
    "watch": "babel -w src --out-dir build",
    "test": "cd build && mocha --recursive --exit"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "bcrypt": "^5.0.1",
    "chai": "^4.3.6",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-validator": "^6.14.0",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^1.0.3",
    "knex-migrate": "^1.7.4",
    "mocha": "^9.2.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "shortid": "^2.2.16",
    "supertest": "^6.2.2",
    "winston": "2.4.0",
    "winston-mongodb": "3.0.0"
  }
}
